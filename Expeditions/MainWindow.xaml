<Window x:Class="Expeditions.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Expeditions"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="800">
    <Grid>
        <TabControl HorizontalAlignment="Left" Height="500" Margin="10,10,0,0" VerticalAlignment="Top" Width="350" Name="Tabs">
        </TabControl>
        <Button Content="Load Adventurer" HorizontalAlignment="Left" Margin="688,491,0,0" VerticalAlignment="Top" Width="96" Click="Button_Load"/>
        <Button Content="Adventure" HorizontalAlignment="Left" Margin="608,491,0,0" VerticalAlignment="Top" Width="75" Click="Button_Adventure"/>
        <Button Content="Save Adventurers" HorizontalAlignment="Left" Margin="688,466,0,0" VerticalAlignment="Top" Width="96" Click="Button_Save"/>
        <ComboBox HorizontalAlignment="Left" Margin="563,442,0,0" VerticalAlignment="Top" Width="120" Name="DestinationBox"/>
    </Grid>
</Window>
